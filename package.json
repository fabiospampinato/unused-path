{
  "name": "unused-path",
  "description": "Reliably get an unused path and copy/move/write to it.",
  "version": "1.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/unused-path/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/unused-path.git"
  },
  "keywords": [
    "get",
    "copy",
    "write",
    "move",
    "unused",
    "path"
  ],
  "dependencies": {
    "copy-unused-path": "^1.5.0",
    "get-unused-path": "^1.4.0",
    "move-unused-path": "^1.4.0",
    "write-unused-path": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
